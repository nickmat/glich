// null001.glcs  Test null expressions

let n = null;

writeln "";

writeln n +", "+ (n = null) +", "+ (null = n) +", "+ (n <> null) +", "+ (n <> null);
writeln (n = 1) +", "+ (1 = n) +", "+ (n <> 1) +", "+ (n <> 1);
writeln (n > null);
writeln (n < null);
writeln (n > 1);
writeln (n < 1);

