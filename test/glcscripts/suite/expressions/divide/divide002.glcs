// divide002.glcs Test file to test division of negative fields

let a = 4 div 2;
let b = 3 div 2;
let c = 2 div 2;
let d = 1 div 2;
let e = 0 div 2;
let f = -1 div 2;
let g = -2 div 2;
let h = -3 div 2;
let i = -4 div 2;

write a + ", " + b + ", " + c + ", " + d + ", " + e
  + ", " + f + ", " + g + ", " + h + ", " + i;


/*[OUTPUT]
2, 1, 1, 0, 0, -1, -1, -2, -2
[OUTPUT]*/
