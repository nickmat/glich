// mark003.hcs Test file to test repeated mark statement

let a = 1;
let b = infinity;
let c = !!empty;
write nl;
write b, c, @type(a) nl;

mark "test";
set context hics;
set integer field;
let d = 2;
write b, c, @type(a), @type(d) nl;

mark "test";
let e = 3;
write b, c, @type(a), @type(e) nl;
write d;


