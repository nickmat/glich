// do019.glcs Test file to test nested do statement blocks

write "Start ";

let count = 0;
do {
  let count = count + 1;
  let newcount = count;
  write "[";
  do {
    let newcount = newcount - 1;
    write newcount;
    while newcount <> 0;
    write ",";
  };
  write "] ";
  until count = 4;
};

write "end";

/*[OUTPUT]
Start [0] [1,0] [2,1,0] [3,2,1,0] end
[OUTPUT]*/
