// if002.glcs Test if elseif statement

let x = 0;

if true
    x = 3;
elseif true
    x = 10;
else
    x = 50;
endif  
writeln x;

if false
    x = 3;
elseif true
    x = 10;
else
    x = 50;
endif  
writeln x;

if true
    x = 3;
elseif false
    x = 10;
else
    x = 50;
endif  
writeln x;

if false
    x = 3;
elseif  false
    x = 10;
else
    x = 50;
endif  
writeln x;

