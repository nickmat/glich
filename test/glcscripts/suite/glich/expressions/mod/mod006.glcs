// mod005.glcs Test file to test division and mod with infinity.

write nl;

let a = +infinity;
let n = 4;
let q = a /% n;
let r = a % n;
let b = (a = n * q + r);
write b, q, r nl;

let a = -infinity;
let n = 4;
let q = a /% n;
let r = a % n;
let b = (a = n * q + r);
write b, q, r nl;


/*[OUTPUT]

true, +infinity, 0
true, -infinity, 0

[OUTPUT]*/
