// minus001.glcs Test file to test unitary minus (negation) expressions

let a = -3;

let b = - -infinity;

let c = -infinity;

let d = -?;

let e = -(5 .. 10);

let f = -(-10 .. -5 | 14 .. 19);

write a +", "+ b +", "+ c +", "+ d +", "+ e +", "+ f;

clear;

