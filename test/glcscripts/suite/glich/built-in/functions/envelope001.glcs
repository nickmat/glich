// envelope001.glcs - @envelope( value ) // return envelope values.

let a = 123n;
let b = 456f;
let c = 10.5;
let d = "text";
let e = 5..10;
let f = 4..8 | 10..18;
let g = {: 10, 20, 30};
let h = true;
let i = null;
let j = @error( "Whoops" );

write nl;
write @envelope( a ), @envelope( b ), @envelope( c ), @envelope( d ), @envelope( e ),
@envelope( f ), @envelope( g ), @envelope( h ), @envelope( i ), @envelope( j ) nl;

