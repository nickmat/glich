// object_name001.glcs - {:} // create object.

object test {
    values one two three;
}

let o1 = {test};
let o2 = {test 1, 2};
let o3 = {test 1, 2, 3, 4};

write nl;
write o1@size, o2@size, o3@size nl;

let o1 = {:};
let o2 = {: 1, 2};
let o3 = {: 1, 2, 3, 4};

write o1@size, o2@size, o3@size nl;

/*[OUTPUT]

3, 3, 4
0, 2, 4

[OUTPUT]*/
