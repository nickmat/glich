// object-def-01.glcs  Example in dev\man\script\stmts\object.htm.

object complex {
    values real imaginary;
    function add( comp ) {
        if @object( comp ) = "complex" {
            result = {complex real+comp[real], imaginary+comp[imaginary]};
        } else {
            result = @error( "Must have 'complex' argument" );
        };
    };
};

let c1 = {complex 5, 9};
let c2 = c1 @add( {complex 20, 6} );

write nl;
write  c1, c2 nl;
write c1@add( c2 ) nl;
write c1@add( 1 ) nl;


