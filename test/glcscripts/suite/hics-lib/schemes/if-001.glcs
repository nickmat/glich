write @text.if:dmy_d( 2460491 ), @date.if:dmy_d( "23 12 1445" ) nl;
write @text.if:wdmy( 2460491 ), @date.if:wdmy( "Yawm as-Sabt 23 Dhū al-Ḥijjah 1445" ) nl;
write @text.if:wdmy_a( 2460491 ), @date.if:wdmy_a( "Sabt 23 Dhu al-Hijjah 1445" ) nl;
write @text.if:ymd( 2460491 ), @date.if:ymd( "1445 Dhu al-Hijjah 23" ) nl;


