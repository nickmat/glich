// Example ISO input and output calendar schemes

mark example_ios5;

format g:iso_y5 {
    rules iso8601 caldate minus year5;
}

format g:iso_y5set {
    rules iso8601 dateset minus year5;
}

format gw:iso_y5wd {
    rules iso8601 week minus year5;
}

format go:iso_y5d {
    rules iso8601 ordinal minus year5;
}
