// Example ISO input and output calendar schemes

mark example_iso6;

format g:iso_y6 {
    rules iso8601 caldate minus year6;
}

format g:iso_y6set {
    rules iso8601 dateset minus year6;
}

format gw:iso_y6wd {
    rules iso8601 week minus year6;
}

format go:iso_y6d {
    rules iso8601 ordinal minus year6;
}
